{
  "$schema": "node_modules/@lerna-lite/cli/schemas/lerna-schema.json",
  "npmClient": "pnpm",
  "version": "independent",
  "command": {
    "publish": {
      "noPrivate": true,
      "conventionalCommits": true,
      "message": "chore(release): publish [skip ci]",
      "changelogPreset": "conventionalcommits"
    }
  },
  "packages": ["codegen", "packages/*", "clients/*"]
}
