{
  "name": "@sp-api-sdk/{{packageName}}",
  "author": "Vincent Mesquita <vincent.mesquita@bizon.solutions>",
  "description": "{{description}}",
  "version": "{{version}}",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "build:cjs": "tsc -p tsconfig.json",
    "build:es": "tsc -p tsconfig.es.json",
    "build": "yarn build:cjs && yarn build:es",
    "clean": "rimraf dist",
    "test": "NODE_ENV='test' yarn jest"
  },
  "dependencies": {
    "@sp-api-sdk/auth": "^{{dependencies.auth}}",
    "@sp-api-sdk/common": "^{{dependencies.common}}",
    "axios": "^0.24.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bizon/selling-partner-api-sdk.git",
    "directory": "clients/{{packageName}}"
  },
  "bugs": {
    "url": "https://github.com/bizon/selling-partner-api-sdk/issues"
  },
  "homepage": "https://github.com/bizon/selling-partner-api-sdk/tree/master/clients/{{packageName}}",
  "keywords": [
    "amazon",
    "bizon",
    "marketplace web services",
    "mws",
    "selling partner api",
    "sp api",
    "sp sdk",
    "{{apiName}}"
  ]
}
