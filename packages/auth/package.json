{
  "name": "@sp-api-sdk/auth",
  "author": "Bertrand Marron <bertrand@bizon.solutions>",
  "description": "Amazon Selling Partner API authentication package",
  "version": "2.2.10",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "xo": "xo",
    "check:ts": "tsc --noEmit",
    "build:cjs": "tsc -p tsconfig.json",
    "build:es": "tsc -p tsconfig.es.json",
    "build": "pnpm build:cjs && pnpm build:es",
    "clean": "rimraf dist",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "read-pkg-up": "^7.0.1"
  },
  "devDependencies": {
    "@sp-api-sdk/jest": "workspace:*",
    "@sp-api-sdk/typescript-config": "workspace:*",
    "nock": "^14.0.10",
    "type-fest": "^4.40.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bizon/selling-partner-api-sdk.git",
    "directory": "packages/auth"
  },
  "bugs": {
    "url": "https://github.com/bizon/selling-partner-api-sdk/issues"
  },
  "homepage": "https://github.com/bizon/selling-partner-api-sdk/tree/master/packages/auth",
  "keywords": [
    "bizon",
    "amazon",
    "auth",
    "spa sdk",
    "sp api",
    "mws",
    "selling partner api",
    "marketplace webservice"
  ],
  "xo": {
    "extends": [
      "xo-bizon"
    ],
    "settings": {
      "import/internal-regex": "^@sp-api-sdk/"
    },
    "semicolon": false,
    "space": 2,
    "prettier": true
  },
  "prettier": {
    "printWidth": 100,
    "jsxSingleQuote": true
  }
}
