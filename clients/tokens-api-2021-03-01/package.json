{
  "name": "@sp-api-sdk/tokens-api-2021-03-01",
  "author": "Bertrand Marron <bertrand@bizon.solutions>",
  "description": "The Selling Partner API for Tokens provides a secure way to access a customer's PII (Personally Identifiable Information). You can call the Tokens API to get a Restricted Data Token (RDT) for one or more restricted resources that you specify. The RDT authorizes subsequent calls to restricted operations that correspond to the restricted resources that you specified. For more information, see the Tokens API Use Case Guide.",
  "version": "3.0.7",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "xo": "xo",
    "check:ts": "tsc --noEmit",
    "build:cjs": "tsc -p tsconfig.json",
    "build:es": "tsc -p tsconfig.es.json",
    "build": "pnpm build:cjs && pnpm build:es",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@sp-api-sdk/common": "workspace:*",
    "axios": "^1.11.0"
  },
  "devDependencies": {
    "@sp-api-sdk/typescript-config": "workspace:*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bizon/selling-partner-api-sdk.git",
    "directory": "clients/tokens-api-2021-03-01"
  },
  "bugs": {
    "url": "https://github.com/bizon/selling-partner-api-sdk/issues"
  },
  "homepage": "https://github.com/bizon/selling-partner-api-sdk/tree/master/clients/tokens-api-2021-03-01",
  "keywords": [
    "amazon",
    "bizon",
    "marketplace web services",
    "mws",
    "selling partner api",
    "sp api",
    "sp sdk",
    "tokens api"
  ],
  "xo": {
    "extends": [
      "xo-bizon"
    ],
    "settings": {
      "import/internal-regex": "^@sp-api-sdk/"
    },
    "semicolon": false,
    "space": 2,
    "prettier": true,
    "ignores": [
      "src/api-model/**"
    ],
    "rules": {
      "unicorn/prevent-abbreviations": "off"
    }
  },
  "prettier": {
    "printWidth": 100,
    "jsxSingleQuote": true
  }
}
