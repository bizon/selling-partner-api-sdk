    "jest-junit": "^16.0.0",
    "rimraf": "^6.1.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "turbo": "^2.8.3",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "xo": "^1.2.3"
  },
