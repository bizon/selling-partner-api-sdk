{
  "name": "@sp-api-sdk/fba-inventory-api-v1",
  "author": "Vincent Mesquita <vincent.mesquita@bizon.solutions>",
  "description": "The Selling Partner API for FBA Inventory lets you programmatically retrieve information about inventory in Amazon&#39;s fulfillment network. Today this API is available only in the North America region. In 2021 we plan to release this API in the Europe and Far East regions.",
  "version": "1.4.0",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:cjs": "tsc -p tsconfig.json",
    "build:es": "tsc -p tsconfig.es.json",
    "build": "yarn build:cjs && yarn build:es",
    "clean": "rimraf dist",
    "test": "NODE_ENV='test' yarn jest"
  },
  "dependencies": {
    "@aws-sdk/client-sts": "^3.16.0",
    "@sp-api-sdk/auth": "^0.3.2",
    "@sp-api-sdk/common": "^0.6.0",
    "axios": "^0.21.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bizon/selling-partner-api-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/bizon/selling-partner-api-sdk/issues"
  },
  "homepage": "https://github.com/bizon/selling-partner-api-sdk/tree/master/clients/fba-inventory-api-v1",
  "keywords": [
    "bizon",
    "amazon",
    "fba inventory api",
    "spa sdk",
    "sp api",
    "mws",
    "selling partner api",
    "marketplace webservice"
  ]
}
