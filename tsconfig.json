{
  "extends": "@tsconfig/node14/tsconfig.json",
  "compilerOptions": {
    "alwaysStrict": true,
    "declaration": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "noEmitHelpers": true,
    "incremental": true,
    "lib": [
      "ES2020",
      "DOM"
    ]
  },
  "typedocOptions": {
    "name": "Selling Partner API SDK for JavaScript",
    "out": "docs",
    "readme": "README.md",
    "excludePrivate": true,
    "excludeExternals": true,
    "entryPointStrategy": "packages",
    "entryPoints": [
      "packages/*",
      "clients/*"
    ],
    "internalNamespace": "<internal>"
  }
}
